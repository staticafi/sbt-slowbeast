<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.4//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.4.dtd">
<benchmark tool="slowbeast" timelimit="60 s" memorylimit="8 GB" cpuCores="1">

<option name="-se"/>
<option name="-no-output"/>

<rundefinition name="sv-comp19_prop-reachsafety">
 <tasks name="ReachSafety-BitVectors">
    <includesfile>/var/tmp/sv-benchmarks/c/ReachSafety-BitVectors.set</includesfile>
    <propertyfile>/var/tmp/sv-benchmarks/c/properties/unreach-call.prp</propertyfile>
  </tasks>
  <tasks name="ReachSafety-ControlFlow">
    <includesfile>/var/tmp/sv-benchmarks/c/ReachSafety-ControlFlow.set</includesfile>
    <propertyfile>/var/tmp/sv-benchmarks/c/properties/unreach-call.prp</propertyfile>
  </tasks>
 <tasks name="ReachSafety-Loops">
    <includesfile>/var/tmp/sv-benchmarks/c/ReachSafety-Loops.set</includesfile>
    <propertyfile>/var/tmp/sv-benchmarks/c/properties/unreach-call.prp</propertyfile>
  </tasks>
 <tasks name="ReachSafety-Recursive">
    <includesfile>/var/tmp/sv-benchmarks/c/ReachSafety-Recursive.set</includesfile>
    <propertyfile>/var/tmp/sv-benchmarks/c/properties/unreach-call.prp</propertyfile>
  </tasks>
</rundefinition>
</benchmark>
